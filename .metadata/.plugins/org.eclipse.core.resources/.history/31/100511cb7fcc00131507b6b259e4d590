package com.edu.bean;

public class SQL_Person {
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getSex() {
		return sex;
	}
	public void setSex(String sex) {
		this.sex = sex;
	}
	public String getPhoto() {
		return photo;
	}
	public void setPhoto(String photo) {
		this.photo = photo;
	}
	public int getGreyear() {
		return Greyear;
	}
	public void setGreyear(int greyear) {
		Greyear = greyear;
	}
	public int getGremouth() {
		return Gremouth;
	}
	public void setGremouth(int gremouth) {
		Gremouth = gremouth;
	}
	public int getGreday() {
		return Greday;
	}
	public void setGreday(int greday) {
		Greday = greday;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getAnimal() {
		return animal;
	}
	public void setAnimal(String animal) {
		this.animal = animal;
	}
	public String getConstellation() {
		return constellation;
	}
	public void setConstellation(String constellation) {
		this.constellation = constellation;
	}
	public String getBeizhuInfo() {
		return beizhuInfo;
	}
	public void setBeizhuInfo(String beizhuInfo) {
		this.beizhuInfo = beizhuInfo;
	}
	//姓名
	private String name;
	//年龄
	private int age;
	//性别
	private String sex;
	//头像
	private String photo;
	//公历年
	private int Greyear;
	//公历月
	private int Gremouth;
	//公历日
	private int Greday;
	//电话
	private String phone;
	//生肖
	private String animal;
	//星座
	private String constellation;
	//备注
	private String beizhuInfo;
	
	
}
